<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.breaktheice.moimat.persistence.LoginMapper">
	
	<select id="login" parameterType="com.breaktheice.moimat.domain.LoginDomain" 
			resultType="com.breaktheice.moimat.domain.LoginDomain">
	 	SELECT	
				mem_id			<!--  LoginDomain.memId 변수명을 찾아서 들어간다 -->
				,mem_email
				,mem_password
		FROM	
				MEMBER 
		WHERE
				mem_email	=	#{memEmail} 

	</select>
	
</mapper>